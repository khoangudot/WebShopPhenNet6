<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Manager Dashboard</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/manager_assets/images/logo/favicon.png">

    <!-- page css -->

    <!-- Core css -->
    <link href="~/manager_assets/css/app.min.css" rel="stylesheet">
</head>

<body>
    <div class="app">
        <div class="layout">
            <!-- Header START -->
            @await Html.PartialAsync("_HeaderPatial")
              
            <!-- Header END -->

            <!-- Side Nav START -->
            @await Html.PartialAsync("_SideNavPartial")
           
            <!-- Side Nav END -->
           
            <!-- Page Container START -->
            <div class="page-container">
                <!-- Content Wrapper START -->
                @RenderBody();             
                <!-- Content Wrapper END -->           
            </div>
            <!-- Page Container END -->        
        </div>
    </div>


    <!-- Core Vendors JS -->
    <script src="~/manager_assets/js/vendors.min.js"></script>

    <!-- page js -->
    <script src="~/manager_assets/vendors/chartjs/Chart.min.js"></script>
    <script src="~/manager_assets/js/pages/dashboard-default.js"></script>

    <!-- Core JS -->
    <script src="~/manager_assets/js/app.min.js"></script>

</body>

</html>